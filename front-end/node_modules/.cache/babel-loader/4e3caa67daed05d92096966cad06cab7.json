ontent":["<template>\n  <div>\n    <h3>注册页</h3>\n    <el-form ref=\"form\" :model=\"form\" label-width=\"80px\">\n      <el-form-item label=\"用户名\">\n        <el-input v-model=\"form.name\"></el-input>\n      </el-form-item>\n      <el-form-item label=\"密码\">\n        <el-input v-model=\"form.password\"></el-input>\n      </el-form-item>\n      <el-form-item label=\"性别\">\n        <el-select v-model=\"form.gender\" placeholder=\"\">\n          <el-option label=\"男\" value=\"1\"></el-option>\n          <el-option label=\"女\" value=\"2\"></el-option>\n        </el-select>\n      </el-form-item>\n      <el-form-item label=\"头像\">\n        <el-upload\n            action=\"http://127.0.0.1:3000/upload/\"\n            list-type=\"picture-card\"\n            :on-preview=\"handlePictureCardPreview\"\n            :on-success=\"handleAvatarSuccess\"\n            :on-remove=\"handleRemove\">\n          <i class=\"el-icon-plus\"></i>\n        </el-upload>\n        <el-dialog :visible.sync=\"dialogVisible\">\n          <img width=\"100%\" :src=\"dialogImageUrl\" alt=\"\">\n        </el-dialog>\n      </el-form-item>\n      <el-form-item label=\"个人简介\">\n        <el-input type=\"textarea\" v-model=\"form.bio\"></el-input>\n      </el-form-item>\n      <el-form-item>\n        <el-button type=\"primary\" @click=\"onSubmit\">注册</el-button>\n        <el-button>取消</el-button>\n      </el-form-item>\n    </el-form>\n  </div>\n</template>\n<script>\n  import request from '../request/api'\n  export default {\n    data() {\n      return {\n        form: {},\n        dialogImageUrl: '',\n        dialogVisible: false\n      }\n    },\n    methods: {\n      onSubmit() {\n        request.signup(this.form).then(res=> {\n          if(res.status === 200) {\n            this.$message.success('恭喜宁，注册成功～')\n            this.$router.push('/signin')\n          }\n        })\n      },\n      handleRemove(file, fileList) {\n        console.log(file, fileList);\n      },\n      handleAvatarSuccess(res) {\n        console.log(res.data.path)\n        this.form.headImg = res.data.path\n      },\n      handlePictureCardPreview(file) {\n        this.dialogImageUrl = file.url;\n        this.dialogVisible = true;\n      }\n    }\n  }\n</script>"],"sourceRoot":"src/components"}]}